{
  "expo": {
    "name": "Task Reminder",
    "slug": "task-reminder",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "android": {
      "permissions": [
        "NOTIFICATIONS"
      ],
      "package": "com.nasimak.taskreminder"
    },
    "ios": {
      "bundleIdentifier": "com.nasimak.taskreminder",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "owner": "nasimak",
    "extra": {
      "eas": {
        "projectId": "4d2ab53e-8a89-417c-84e7-c11aeb5922ba"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": ["./assets/ringtone.wav"]
        }
      ]
    ]
  }
}
